# AgenticAI V4 – variables d'environnement exemple
# Copier ce fichier en `.env` et ajuster les valeurs selon l'environnement.

# API
APP_ENV=local
API_HOST=0.0.0.0
API_PORT=8000

# Base de données relationnelle
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=agenticai
POSTGRES_USER=agenticai
POSTGRES_PASSWORD=agenticai

# Vector DB
VECTOR_DB=qdrant
VECTOR_URL=http://localhost:6333

# MinIO / stockage objets
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=agenticai
MINIO_SECRET_KEY=agenticai

# Messaging
REDIS_URL=redis://localhost:6379/0

# Ollama
OLLAMA_HOST=http://localhost:11434

# Sécurité / Vault
VAULT_ADDR=http://localhost:8200
